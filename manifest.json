{
    "manifest_version": 3,
    "name": "BlockIt",
    "version": "1.0.0",
    "author": "T Aatreya-unknown7703(github)",
    "description": "Custom website blocker",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "action": {
      "default_icon": {
        "16": "icons/icon16.png",
        "24": "icons/icon24.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      },
      "default_title": "BlockIt",
      "default_popup": "popup.html"
    },
    "permissions": [
      "storage",
      "tabs",
      "webNavigation",  
      "scripting"       
    ],
    "background": {
      "service_worker": "background.js"
    },
    "options_page": "options.html",
    "short_name": "BlockIt"
  }
  